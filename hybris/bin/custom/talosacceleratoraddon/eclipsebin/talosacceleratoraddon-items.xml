    <itemtypes>
        <typegroup name="TalosCommerce">
            <itemtype code="TCSocialAction"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCSocialAction"
                      extends="SimpleCMSAction">
                <attributes>
                    <attribute qualifier="iconCode" type="SocialIconType">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="localizedUrl" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TCSocialCollectionComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCSocialCollectionComponent"
                      extends="SimpleCMSComponent"/>

            <itemtype code="TCEmailOptInComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCEmailOptInComponent"
                      extends="SimpleCMSComponent"/>

            <itemtype code="FooterNavigationComponent" autocreate="false" generate="false">
                <attributes>
                    <attribute qualifier="optInComponent" type="TCEmailOptInComponent">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="socialCollectionComponent" type="TCSocialCollectionComponent">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TCBannerAction"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCBannerAction"
                      extends="SimpleCMSAction">
                <attributes>
                    <attribute qualifier="text" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="style" type="TCButtonStyle">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>em().getEnumerationValue("TCButtonStyle","BTN_DEFAULT")</defaultvalue>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TfResponsiveBannerComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TfResponsiveBannerComponent"
                      extends="SimpleBannerComponent">
                <attributes>
                    <attribute qualifier="backgroundImage" type="Media">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="subtitle" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TCResponsiveBannerComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCResponsiveBannerComponent"
                      extends="SimpleCMSComponent">
                <attributes>
                    <attribute qualifier="contentSpacing" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="maxContentWidth" type="java.lang.String">
                        <persistence type="property"/>
                        <defaultvalue>"1170px"</defaultvalue>
                    </attribute>
                    <attribute qualifier="gridColumns" type="java.lang.Integer">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>4</defaultvalue>
                    </attribute>

                    <attribute qualifier="titleXs" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="titleSm" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="titleMd" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="titleLg" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>

                    <attribute qualifier="descriptionXs" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="descriptionSm" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="descriptionMd" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>
                    <attribute qualifier="descriptionLg" type="localized:java.lang.String">
                        <persistence type="property"/>
                        <modifiers dontOptimize="true"/>
                    </attribute>

                    <attribute qualifier="mediaXs" type="Media">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="mediaSm" type="Media">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="mediaMd" type="Media">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="mediaLg" type="Media">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="showContentOnHoverXs" type="java.lang.Boolean">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                    </attribute>
                    <attribute qualifier="showContentOnHoverSm" type="java.lang.Boolean">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                    </attribute>
                    <attribute qualifier="showContentOnHoverMd" type="java.lang.Boolean">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                    </attribute>
                    <attribute qualifier="showContentOnHoverLg" type="java.lang.Boolean">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                    </attribute>


                    <attribute qualifier="heightXs" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="heightSm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="heightMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="heightLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="backgroundSizeXs" type="java.lang.String">
                        <persistence type="property"/>
                        <defaultvalue>"auto 100%"</defaultvalue>
                    </attribute>
                    <attribute qualifier="backgroundSizeSm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundSizeMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundSizeLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="backgroundPositionXs" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundPositionSm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundPositionMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundPositionLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="flexDirectionXs" type="FlexDirectionEnum">
                        <persistence type="property"/>
                        <defaultvalue>em().getEnumerationValue("FlexDirectionEnum","COLUMN")</defaultvalue>
                    </attribute>
                    <attribute qualifier="flexDirectionSm" type="FlexDirectionEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="flexDirectionMd" type="FlexDirectionEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="flexDirectionLg" type="FlexDirectionEnum">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="justifyContentXs" type="JustifyContentEnum">
                        <persistence type="property"/>
                        <defaultvalue>em().getEnumerationValue("JustifyContentEnum","CENTER")</defaultvalue>
                    </attribute>
                    <attribute qualifier="justifyContentSm" type="JustifyContentEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="justifyContentMd" type="JustifyContentEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="justifyContentLg" type="JustifyContentEnum">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="alignItemsXs" type="AlignItemsEnum">
                        <persistence type="property"/>
                        <defaultvalue>em().getEnumerationValue("AlignItemsEnum","CENTER")</defaultvalue>
                    </attribute>
                    <attribute qualifier="alignItemsSm" type="AlignItemsEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="alignItemsMd" type="AlignItemsEnum">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="alignItemsLg" type="AlignItemsEnum">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="backgroundColorXs" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundColorSm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundColorMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="backgroundColorLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="overlayXs" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="overlaySm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="overlayMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="overlayLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="textColorXs" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="textColorSm" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="textColorMd" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="textColorLg" type="java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TCRotatingBannerComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCRotatingBannerComponent"
                      extends="SimpleCMSComponent">
                <attributes>
                    <attribute qualifier="title" type="localized:java.lang.String">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="banners" type="TCResponsiveBannerComponentList">
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="navColor" type="java.lang.String">
                        <persistence type="property"/>
                        <defaultvalue>"#252525"</defaultvalue>
                    </attribute>

                    <attribute qualifier="itemsShownXs" type="java.lang.Integer">
                        <persistence type="property"/>
                        <defaultvalue>1</defaultvalue>
                    </attribute>
                    <attribute qualifier="itemsShownSm" type="java.lang.Integer">
                        <persistence type="property"/>
                        <defaultvalue>1</defaultvalue>
                    </attribute>
                    <attribute qualifier="itemsShownMd" type="java.lang.Integer">
                        <persistence type="property"/>
                        <defaultvalue>1</defaultvalue>
                    </attribute>
                    <attribute qualifier="itemsShownLg" type="java.lang.Integer">
                        <persistence type="property"/>
                        <defaultvalue>1</defaultvalue>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="ProductCarouselComponent" autocreate="false" generate="false">
                <attributes>
                    <attribute qualifier="navColor" type="java.lang.String">
                        <persistence type="property"/>
                        <defaultvalue>"#F2F2F2"</defaultvalue>
                    </attribute>
                    <attribute qualifier="backgroundColor" type="java.lang.String">
                        <persistence type="property"/>
                        <defaultvalue>"#252525"</defaultvalue>
                    </attribute>
                    <attribute qualifier="rotateActiveItem" type="java.lang.Boolean">
                        <persistence type="property"/>
                        <modifiers optional="false"/>
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TCResponsiveGridComponent"
                      jaloclass="com.talos.talosacceleratoraddon.jalo.TCResponsiveGridComponent"
                      extends="SimpleCMSComponent">
                <attributes>
                    <attribute qualifier="gridItems" type="TCResponsiveBannerComponentList">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CMSNavigationNode" autocreate="false"
                      generate="true"
                      extends="CMSNavigationNode">
                <attributes>
                    <attribute qualifier="banner" type="TCResponsiveBannerComponent">
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
    </itemtypes>
</items>
