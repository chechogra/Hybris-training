    <enumtypes>
        <enumtype code="SocialIconType" dynamic="true">
            <value code="facebook"/>
            <value code="twitter"/>
            <value code="instagram"/>
            <value code="gplus"/>
            <value code="youtube"/>
            <value code="linkedin"/>
